{
  "background": {
    "persistent": false,
    "scripts": ["js/underscore-min.js", "js/dictionary.js", "js/background.js"]
  },
  "content_scripts": [{
    "all_frames": true,
    "css": ["css/fullScreener.css"],
    "js": ["js/fullScreener.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "default_locale": "ru",
  "description": "__MSG_description__",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "default_locale": "ru",
  "permissions": ["bookmarks", "tabs", "contextMenus", "http://*/*", "https://*/*"],
  "version": "1.0.0"
}
